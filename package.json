{
  "name": "israel-exit-survey",
  "version": "0.1.0",
  "bin": {
    "israel-exit-survey": "bin/israel-exit-survey.js"
  },
  "scripts": {
    "build": "npm run lint && node build-layers && tsc",
    "build-no-layers": "npm run lint && tsc",
    "cdk": "cdk",
    "lint": "eslint . --ignore-path .gitignore",
    "package-upgrade": "node package-upgrade",
    "synth": "npm run build && cdk synth",
    "synth-no-layers": "npm run build-no-layers && echo 'performing cdk synthesis...' && cdk synth",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "@types/source-map-support": "^0.5.4",
    "aws-cdk-lib": "2.35.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "fs-extra": "^10.1.0",
    "aws-cdk": "2.35.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  }
}
